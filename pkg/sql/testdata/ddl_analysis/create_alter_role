# CreateRole
count
CREATE ROLE rolea
----
6

count
CREATE ROLE roleb LOGIN
----
6

count
CREATE ROLE rolec LOGIN CREATEROLE
----
8

count
CREATE ROLE roled LOGIN CREATEROLE VALID UNTIL '2021-01-01'
----
10

# AlterRole
count
